--
--  compile to JSON with 'iotql-model KnxLight.iotql'
--
CREATE MODEL KnxLight WITH
    schema:name = "KnxLight",
    schema:description = "On/Off Light",
    iot:facet = iot-facet:lighting
ATTRIBUTE on WITH
    schema:name = "on",
    iot:purpose = iot-purpose:on,
    iot:type = iot:type.boolean
;
